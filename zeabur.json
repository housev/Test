{
    "name": "whatsapp-bot",
    "services": {
        "bot": {
            "build": {
                "engine": "nodejs",
                "command": "npm install"
            },
            "run": {
                "command": "node main.js --no-sandbox --disable-setuid-sandbox"
            },
            "env": [
                "PORT=3000",
                "SESSION=your_session_here"
            ]
        }
    }
}